{
  "name": "should-release",
  "version": "1.2.0",
  "description": "Minimalistic command line tool to check if a new release should to be published",
  "type": "module",
  "bin": "cli.js",
  "scripts": {
    "lint": "eslint *.js",
    "lint:fix": "eslint . --fix",
    "should-release": "node ./cli.js",
    "release": "standard-version"
  },
  "keywords": [
    "semver",
    "release",
    "version",
    "versioning"
  ],
  "main": "index.js",
  "repository": "saintedlama/should-release",
  "author": "Christoph Walcher <christoph.walcher@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "conventional-recommended-bump": "^6.1.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.5.0",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.6.0",
    "prettier": "^3.3.2",
    "standard-version": "^9.3.2"
  }
}
